<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/quickLinks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="149dp"
        android:text="Quick Links"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/homepage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/quickLinks"
        android:layout_below="@+id/quickLinks"
        android:layout_marginTop="21dp"
        android:text="WVU Tech Homepage" />

    <TextView
        android:id="@+id/bookstoreL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/homepage"
        android:layout_below="@+id/homepage"
        android:layout_marginTop="13dp"
        android:text="Barnes and Noble at Tech" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/quickLinks"
        android:layout_marginTop="22dp"
        android:text="Welcome back to TecStart Mobile!"
        android:textSize="18sp" />
</RelativeLayout>